<%- include("partials/header") %>

<div class="container mx-auto p-4">
    <div class="bg-gray-800 p-8 rounded-lg shadow-lg">
        <h2 class="text-3xl font-bold text-white mb-6">Token Created Successfully!</h2>
        <div class="space-y-4">
            <div>
                <label class="block text-sm font-medium text-gray-300">Token Name:</label>
                <p class="mt-1 text-lg font-semibold text-white"><%= tokenName %></p>
            </div>
            <div>
                <label class="block text-sm font-medium text-gray-300">Token Symbol:</label>
                <p class="mt-1 text-lg font-semibold text-white"><%= tokenSymbol %></p>
            </div>
            <div>
                <label class="block text-sm font-medium text-gray-300">Token URI:</label>
                <p class="mt-1 text-lg font-semibold text-white"><%= tokenURI %></p>
            </div>
            <div>
                <label class="block text-sm font-medium text-gray-300">Token Decimals:</label>
                <p class="mt-1 text-lg font-semibold text-white"><%= tokenDecimals %></p>
            </div>
            <div>
                <label class="block text-sm font-medium text-gray-300">Initial Supply:</label>
                <p class="mt-1 text-lg font-semibold text-white"><%= tokenInitialSupply %></p>
            </div>
            <div>
                <label class="block text-sm font-medium text-gray-300">Mint Address:</label>
                <p class="mt-1 text-lg font-semibold text-white"><%= mintAddress %></p>
            </div>
            <div>
                <label class="block text-sm font-medium text-gray-300">Token Account Address:</label>
                <p class="mt-1 text-lg font-semibold text-white"><%= fromTokenAccount %></p>
            </div>
        </div>
        <div class="mt-6">
            <a href="/" class="inline-flex justify-center py-3 px-4 border border-transparent shadow-sm text-sm font-medium rounded-md text-white bg-blue-600 hover:bg-blue-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-blue-500">
                Create Another Token
            </a>
        </div>
    </div>
</div>

<%- include("partials/footer") %>